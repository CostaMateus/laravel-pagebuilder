{
    "name": "juhara/laravel-pagebuilder",
    "description": "A drag and drop pagebuilder to manage pages in any Laravel project.",
    "keywords": [
        "laravel",
        "pagebuilder"
    ],
    "homepage": "https://github.com/zamronypj/Laravel-Pagebuilder",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans Schouten",
            "email": "hans@falcosolutions.nl"
        },
        {
            "name": "Zamrony P. Juhara",
            "email": "zamronypj@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "hansschouten/phpagebuilder": "^0.24"
    },
    "autoload": {
        "psr-4": {
            "Juhara\\LaravelPageBuilder\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Juhara\\LaravelPageBuilder\\ServiceProvider"
            ],
            "aliases": {
                "PageBuilder": "Juhara\\LaravelPageBuilder\\Facade"
            }
        }
    }
}
